<UserControl x:Class="TestGenerator.SettingsPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TestGenerator"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <TextBox x:Name="TestFramework" HorizontalAlignment="Left" Height="23" Margin="26,26,0,0" Grid.Row="1" TextWrapping="Wrap" Text="Test Framework" VerticalAlignment="Top" Width="120" TextChanged="Framework_Changed"/>
    </Grid>
</UserControl>
